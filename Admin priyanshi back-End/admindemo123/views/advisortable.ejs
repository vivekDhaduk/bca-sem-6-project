  <div class="main-content" id="panel">
    <!-- Topnav -->
    <%- include('top'); -%>
    <!-- Header -->
    <!-- Header -->
    <div class="header  pb-6">
      <div class="container-fluid">
        <div class="header-body">
          <div class="row align-items-center py-4">
            <div class="col-lg-6 col-7">
              <h6 class="h2 text-white d-inline-block mb-0" >Advisor Tables</h6>
             
            </div>
            <div class="col-lg-6 col-5 text-right">
              <a href="/adviser/insert" class="btn btn-big btn-neutral text-black-50" style="background-color: #fff;">adviser insert</a>
              <!-- <a href="#" class="btn btn-sm btn-neutral">Filters</a> -->
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Page content -->
    <div class="container">
      <div class="row">
        <div class="col">
          <div class="card">
            <!-- Card header -->
           
            <!-- Light table -->
            <div class="table-responsive">
                <% if(records != ''){ %>
                  <table id="example" class="table table-striped" style="width:100%">
                <thead class="thead-light">
                  <tr>
                   <th scope="col" class="sort" data-sort="name">image</th>
                    <th scope="col" class="sort" data-sort="name">name</th>
                    <th scope="col" class="sort" data-sort="email">category</th>
                    <th scope="col" class="sort" data-sort="number">number</th>
                    <th scope="col" class="sort" data-sort="gender">gender</th>
                    <th scope="col" class="sort" data-sort="age">discription</th>
                    <th scope="col" class="sort" data-sort="action">action</th>
                  </tr>
                </thead>
                <tbody class="list">
                    <% records.forEach(function(row){%>
                  <tr>
                    <td><img src="<%= row.image %>" width="50" class="img-thumbnail"></td>
                    <td><%= row.name %></td>
                    <td><%= row.category %></td>
                    <td><%= row.number %></td>
                    <td><%= row.gender %></td>
                    <td><%= row.discription %></td>
<!--                 <td><img src="<%= row.image %>" width="50" class="img-thumbnail"></td> --> 
                    <td><a class="btn btn-primary btn-xs" href="/adviser/edit/<%= row._id %> "><i class="fa fa-edit"></i></a>
                    <a class="btn btn-danger btn-xs" href="/adviser/delete/<%= row._id %>"><i class="fa fa-trash"></i></a>
                    <a class="btn btn-secondary btn-xs" href="/adviser/viewprofile/<%= row._id %>"><i class="fas fa-eye"></i></a></td>
                                 
                    </tr>
                    <% }) %> 
                 
                </tbody>
              </table><% } else{ %>
                <h1 class="text-center text-secondary">user not found</h1>
                <% } %>
            </div>
            <!-- Card footer -->
            <div class="card-footer py-4">
              <nav aria-label="...">
              <!--   <ul class="pagination justify-content-end mb-0">
                  <li class="page-item disabled">
                    <a class="page-link" href="#" tabindex="-1">
                      <i class="fas fa-angle-left"></i>
                      <span class="sr-only">Previous</span>
                    </a>
                  </li>
                  <li class="page-item active">
                    <a class="page-link" href="/admin/view">1</a>
                  </li>
                  <li class="page-item">
                    <a class="page-link" href="/adviser/view">2 <span class="sr-only">(current)</span></a>
                  </li>
                  <li class="page-item"><a class="page-link" href="#">3</a></li>
                  <li class="page-item">
                    <a class="page-link" href="#">
                      <i class="fas fa-angle-right"></i>
                      <span class="sr-only">Next</span>
                    </a>
                  </li>
                </ul> -->
              </nav>
            </div>
          </div>
        </div>
      </div>
     </div>
  </div>
