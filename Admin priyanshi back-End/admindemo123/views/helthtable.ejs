      <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<style>
  .ellipsis {
      overflow: hidden;
      text-overflow: hidden;
      text-overflow: ellipsis; 
      max-height: 10px;
      max-width: 200px;
      -webkit-line-clamp: 2;
    }
</style>
</head>

<body>
  <!-- Sidenav -->
  <%- include('sidebar'); -%>
  <!-- Main content -->
  <div class="main-content" id="panel">
    <!-- Topnav -->
    <%- include('top'); -%>
    <!-- Header -->
    <!-- Header -->
    <div class="header  pb-6">
      <div class="container-fluid">
        <div class="header-body">
          <div class="row align-items-center py-4">
            <div class="col-lg-6 col-7">
              <h6 class="h2 text-white d-inline-block mb-0">Helth Tables</h6>
            </div>
            <div class="col-lg-6 col-5 text-right">
              <a href="/policy/inserthelth" class="btn btn-big btn-neutral text-black-50" style="background-color: #fff;">insert policy</a>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Page content -->
    <div class="container">
      <div class="row">
        <div class="col">
          <div class="card">
            <!-- Card header -->
           
            <!-- Light table -->
            <div class="table-responsive">
                <table id="example" class="table table-striped" style="width:100%">
                    <% if(records != ''){ %>
                <thead >
                  <tr>
                    
                    <th>logo</th>
                    <th>name</th>
                    <th> primumamount</th>
                    <th>timeduration</th>
                    <th>shortdiscription</th>
                    <th>cover</th>
                    <th>longdiscription</th>
                    <th>age</th>
                    <th>action</th>
                  </tr>
                </thead>
                <tbody class="list">
                    <% records.forEach(function(row){%>
                    
                  <tr>
        
                    <td><img src="<%= row.logo%>" width="50" sizes="50"></td> 
                    <td><%= row.name %></td>
                    <td><%= row.primumamount %></td>
                    <td ><%= row.timeduration %></td>
                    <td class="ellipsis"><%= row.shortdiscription %></td>
                    <td><%= row.cover %></td>
                   <td class="ellipsis"><%= row.longdiscription %></td>
                    <td ><%= row.age%></td>
					          <td><a class="btn btn-primary btn-xs" href="/policy/edithelth/<%= row._id %> "><i class="fa fa-edit"></i></a>
                    <a class="btn btn-danger btn-xs" href="/policy/deletehelth/<%= row._id %>"><i class="fa fa-trash"></i></a>
                    <a class="btn btn-secondary btn-xs" href="/policy/viewhelthdetail/<%= row._id %>"><i class="fas fa-eye"></i></a></td>           
                    </tr>
                    <% }) %> 
                 
                </tbody>
             <% } else{ %>
                <h1 class="text-center text-secondary">policy not found</h1>
                <% } %>
            </div>
          </table>
            
          </div>
        </div>
      </div>
     
      
    
  <!-- Argon Scripts -->
  <!-- Core -->
  <script src="/assets/vendor/jquery/dist/jquery.min.js"></script>
  <script src="/assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="/assets/vendor/js-cookie/js.cookie.js"></script>
  <script src="/assets/vendor/jquery.scrollbar/jquery.scrollbar.min.js"></script>
  <script src="/assets/vendor/jquery-scroll-lock/dist/jquery-scrollLock.min.js"></script>
  <!-- Argon JS -->
  <script src="/assets/js/argon.js?v=1.2.0"></script>
  <script>
    $(document).ready(function() {
$('#example').DataTable();
} );
</script>


<script src="/vendors/js/vendor.bundle.base.js"></script>

<script src="/js/off-canvas.js"></script>
<script src="/js/misc.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.9.2/umd/popper.min.js"
