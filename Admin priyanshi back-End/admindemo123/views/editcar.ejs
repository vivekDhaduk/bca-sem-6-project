 
<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="Start your development with a Dashboard for Bootstrap 4.">
  <meta name="author" content="Creative Tim">
  <title>insurance mangment system</title>
  <!-- Favicon -->
  <link rel="icon" href="/assets/img/brand/favicon.png" type="image/png">
  <!-- Fonts -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700">
  <!-- Icons -->
  <link rel="stylesheet" href="/assets/vendor/nucleo/css/nucleo.css" type="text/css">
  <link rel="stylesheet" href="/assets/vendor/@fortawesome/fontawesome-free/css/all.min.css" type="text/css">
  <!-- Argon CSS -->
  <link rel="stylesheet" href="/assets/css/argon.css?v=1.2.0" type="text/css">
</head>

<body class="bg-default">
  <!-- Navbar -->
  <%- include('sidebar'); -%>
  <!-- Main content -->
  <div class="main-content">
    <!-- Header -->
    <div class="header bg-gradient-primary py-7 py-lg-8 pt-lg-9">
      <div class="container">
        <div class="header-body text-center mb-7">
          
          </div>
        </div>
      </div>
      <div class="separator separator-bottom separator-skew zindex-100">
        <svg x="0" y="0" viewBox="0 0 2560 100" preserveAspectRatio="none" version="1.1" xmlns="http://www.w3.org/2000/svg">
          <polygon class="fill-default" points="2560 0 2560 100 0 100"></polygon>
        </svg>
      </div>
    </div>
    <!-- Page content -->
    <div class="container mt--8 pb-5">
      <!-- Table -->
      <div class="row justify-content-center">
        <div class="col-lg-6 col-md-8">
          <div class="card bg-secondary border-0">
            <div class="card-header bg-transparent pb-5">
              <div class="text-muted text-center mt-2 mb-4"><small>insert helth policy</small></div>
              <div class="text-center">
               
              </div>
            </div>
            <div class="card-body px-lg-5 py-lg-5">
             
			<form method="post" action="/policy/updatecar" enctype="multipart/form-data">
                <input type="hidden" name="id" value="<%= records._id %>">
                   <div class="input-group input-group-merge input-group-alternative mb-3">
                  <div class="input-group-prepend">
                  <span class="input-group-text"><i class="ni ni-email-83"></i></span>
                  </div>
                  
                </div>
                </div> 
                <div class="form-group">
                  <div class="input-group input-group-merge input-group-alternative mb-3">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="ni ni-hat-3"></i></span>
                    </div>
                    <input class="form-control" name="name" placeholder="enter name" value="<%= records.name %>" type="text" >
                  </div>
                </div> 
                <div class="form-group">
                  <div class="input-group input-group-merge input-group-alternative mb-3">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="ni ni-hat-3"></i></span>
                    </div>
                    <input class="form-control" name="primumamount" placeholder="enter primumamount" value="<%= records.primumamount %>" type="text" >
                  </div>
                </div>
                <div class="form-group">
                  <div class="input-group input-group-merge input-group-alternative mb-3">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="ni ni-email-83"></i></span>
                    </div>
                    <input class="form-control" name="timeduration" placeholder="enter timeduration" value="<%= records.timeduration%>" type="text">
                  </div>
                </div>
				<div class="form-group">
					<div class="input-group input-group-merge input-group-alternative mb-3">
					  <div class="input-group-prepend">
						<span class="input-group-text"><i class="ni ni-email-83"></i></span>
					  </div>
					  <input class="form-control" placeholder="shortdiscription" name="shortdiscription" value="<%= records.shortdiscription%>" type="text" >
					</div>
				  </div>
					<div class="form-group">
					<div class="input-group input-group-merge input-group-alternative mb-3">
					  <div class="input-group-prepend">
						<span class="input-group-text"><i class="ni ni-email-83"></i></span>
					  </div>
					  <input class="form-control" type="text" placeholder="enter cover" name="cover" value="<%= records.cover%>" >
					</div>
				  </div>
                <div class="form-group">
                  <div class="input-group input-group-merge input-group-alternative">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="ni ni-lock-circle-open"></i></span>
                    </div>
                    <input class="form-control" type="text" placeholder="enter longdiscription" name="longdiscription" value="<%= records.longdiscription%>">
                  </div>
                </div>
              
                  <div class="form-group">
                    <div class="input-group input-group-merge input-group-alternative">
                      <div class="input-group-prepend">
                        <span class="input-group-text"><i class="ni ni-lock-circle-open"></i></span>
                      </div>
                      <input class="form-control" type="text"  name="idv" value="<%= records.idv%>" >
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="input-group input-group-merge input-group-alternative">
                      <div class="input-group-prepend">
                        <span class="input-group-text"><i class="ni ni-lock-circle-open"></i></span>
                      </div>
                      <input class="form-control" type="text"  name="purchaseyear" value="<%= records.purchaseyear%>" >
                    </div>
                  </div>
                <div class="text-center">
                  <button type="submit" class="btn btn-primary mt-4">update policy</button>
				      </div>
              </form>
			  
			
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- Footer -->
<!--   <%- include('footer'); -%> -->
  <!-- Argon Scripts -->
  <!-- Core -->
  <script src="../assets/vendor/jquery/dist/jquery.min.js"></script>
  <script src="../assets/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="../assets/vendor/js-cookie/js.cookie.js"></script>
  <script src="../assets/vendor/jquery.scrollbar/jquery.scrollbar.min.js"></script>
  <script src="../assets/vendor/jquery-scroll-lock/dist/jquery-scrollLock.min.js"></script>
  <!-- Argon JS -->
  <script src="../assets/js/argon.js?v=1.2.0"></script>
</body>

</html>
